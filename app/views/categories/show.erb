<section id="main">
  <div class="inner">
    <div class="table-wrapper">
      <table>
        <thead>
          <tr>
            <th>Title</th>
            <th colspan="3"></th>
          </tr>
        </thead>

        <tbody>
          <%if @questions != nil%>
            <%@questions.each do |a|%>
            <tr>
              <td>
                  <%=link_to a.title, category_question_path(params[:id],a.id)%></br>
              </td>
            </tr>
            <% end %>
          <%end%>
        </tbody>
      </table>
      <%if user_signed_in?%>
      <%= link_to "새로운 질문 하러가기", new_category_question_path(params[:id])%>
      <%end%>
    </div>
  </div>
</section>
