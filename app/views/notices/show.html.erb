<section id="one">
  <div class="inner">
    <p id="notice"><%= notice %></p>

    <p>
      <strong>Title:</strong>
      <%= @notice.title %>
    </p>

    <p>
      <strong>Content:</strong>
      <%= @notice.content %>
    </p>
    <%if current_user.is_admin?%>
    <%= link_to 'Edit', edit_notice_path(@notice) %> |
    <%= link_to 'Back', root_path %>
    <%end%>
  </div>
</section>
